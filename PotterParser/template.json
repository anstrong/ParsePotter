{
  "quiz": {
    "title": {
      "tag": "title",
      "remove": " | Wizarding World"
    },
    "numberContent": {
      "class": "Quiz-content-inner",
      "num_questions": {
        "tag": "span",
        "class": "translate-component",
        "remove": " questions"
      }
    },
    "question": {
      "titleContent": {
        "class": "QuizQuestion-info",
        "title": {
          "tag": "h2",
          "class": "QuizQuestion-info-title"
        }
      },
      "answerList": {
        "class": "QuizQuestion-options-list",
        "answerContent": {
          "class": "QuizQuestion-options",
          "answer": {
            "type": {
              "class": "QuizQuestionOption-input",
              "attribute": "type",
              "options": [
                {
                  "label": "checkbox",
                  "clickables": [
                    "label",
                    "button"
                  ],
                  "prompt": " (Select all that apply)"
                },
                {
                  "label": "radio",
                  "clickables": [
                    "label"
                  ],
                  "prompt": " (Select one of the following)"
                }
              ]
            },
            "option": {
              "class": "QuizQuestion-options",
              "attribute": "class",
              "options": [
                {
                  "label": "text",
                  "id": "item"
                },
                {
                  "label": "image",
                  "id": "image"
                }
              ]
            },
            "label": {
              "tag": "div",
              "class": "QuizQuestionOption"
            }
          }
        }
      }
    }
  }
}
